		"devdoc": {
			"details": "Adds pass/fail logic and allows updating missing subject data",
			"kind": "dev",
			"methods": {
				"addStudent(uint256,string,uint8,uint8,uint8,uint8)": {
					"details": "Add a new student with their marks and evaluate pass/fail status",
					"params": {
						"_englishMarks": "Marks in English (out of 100)",
						"_hindiMarks": "Marks in Hindi (out of 100)",
						"_id": "Student ID",
						"_mathMarks": "Marks in Math (out of 100)",
						"_name": "Student name",
						"_scienceMarks": "Marks in Science (out of 100)"
					}
				},
				"getPassedStudentsCount()": {
					"details": "Get number of students who passed",
					"returns": {
						"_0": "Count of students who passed"
					}
				},
				"getVersion()": {
					"details": "Get version of the contract",
					"returns": {
						"_0": "Contract version string"
					}
				},
				"hasStudentPassed(uint256)": {
					"details": "Check if student has passed",
					"params": {
						"_studentId": "Student ID"
					},
					"returns": {
						"_0": "Student's pass status"
					}
				},
				"updateMarks(uint256,uint8,uint8,uint8,uint8)": {
					"details": "Update all marks for an existing student",
					"params": {
						"_englishMarks": "New marks in English",
						"_hindiMarks": "New marks in Hindi",
						"_mathMarks": "New marks in Math",
						"_scienceMarks": "New marks in Science",
						"_studentId": "Student ID"
					}
				},
				"updateScienceMarks(uint256,uint8)": {
					"details": "Add or update science marks for existing students",
					"params": {
						"_scienceMarks": "Marks in Science",
						"_studentId": "Student ID"
					}
				},
				"upgradeStudentRecord(uint256)": {
					"details": "Update existing student records with missing data from V1/V2 This function can add science marks and update pass/fail status",
					"params": {
						"_studentId": "Student ID"
					}
				}
